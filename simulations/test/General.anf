<?xml version="1.0" encoding="UTF-8"?>
<scave:Analysis xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:scave="http://www.omnetpp.org/omnetpp/scave">
  <inputs>
    <inputs name="/recharge_framework/simulations/test/results/General-*.vec"/>
    <inputs name="/recharge_framework/simulations/test/results/General-*.sca"/>
  </inputs>
  <datasets>
    <datasets name="personal coverage">
      <items xsi:type="scave:Add" filterPattern="personalCoverage" type="VECTOR"/>
      <items xsi:type="scave:LineChart" name="personalCoverage" lineNameFormat=""/>
      <items xsi:type="scave:Apply" operation="aggregator">
        <params name="function" value="average"/>
      </items>
      <items xsi:type="scave:LineChart" name="personalCoverageAverage" lineNameFormat=""/>
    </datasets>
  </datasets>
  <chartSheets>
    <chartSheets name="default" charts="//@datasets/@datasets.0/@items.1 //@datasets/@datasets.0/@items.3"/>
  </chartSheets>
</scave:Analysis>
